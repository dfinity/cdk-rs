{
  "version": 1,
  "canisters": {
    "counter_rs": {
      "type": "rust",
      "candid": "src/counter_rs/counter.did",
      "output": "../../target/wasm32-unknown-unknown/debug/counter_rs.wasm"
    },
    "counter_mo": {
      "type": "motoko",
      "main": "src/counter_mo/main.mo"
    },
    "inter_rs": {
      "type": "rust",
      "candid": "src/inter_rs/lib.did",
      "output": "../../target/wasm32-unknown-unknown/debug/inter_rs.wasm",
      "dependencies": [
        "counter_mo"
      ]
    },
    "inter_mo": {
      "type": "motoko",
      "main": "src/inter_mo/main.mo"
    },
    "inter2_rs": {
      "type": "rust",
      "candid": "src/inter2_rs/lib.did",
      "output": "../../target/wasm32-unknown-unknown/debug/inter2_rs.wasm",
      "dependencies": [
        "inter_mo"
      ]
    },
    "inter2_mo": {
      "type": "motoko",
      "main": "src/inter2_mo/main.mo"
    }
  },
  "defaults": {
    "build": {
      "output": "canisters/"
    }
  }
}
