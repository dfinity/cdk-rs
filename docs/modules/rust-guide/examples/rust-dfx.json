{
    "canisters": {
        "rust_hello": {
            "build": "cargo build --target wasm32-unknown-unknown --package rust_hello --release",
            "candid": "src/rust_hello/rust_hello.did",
            "wasm": "target/wasm32-unknown-unknown/release/rust_hello.wasm",
            "type": "custom"
        }
    },
    "defaults": {
        "build": {
            "packtool": ""
        }
    },
    "dfx": "0.7.0-beta.8",
    "networks": {
        "local": {
            "bind": "127.0.0.1:8000",
            "type": "ephemeral"
        },
        "ic": {
            "providers": [
                "https://gw.dfinity.network"
            ],
            "type": "persistent"
        }
    },
    "version": 1
}